---
import BaseLayout from "../layouts/BaseLayout.astro";
import PaqueteDetalle from "../components/PaqueteDetalle.jsx";

const API_URL = import.meta.env.PUBLIC_API_URL;
const DOMINIO = import.meta.env.PUBLIC_DOMINIO;
---
<BaseLayout title="Detalle del paquete - Coba Viajes" description="Descubre los detalles de nuestros paquetes turísticos" keywords="paquete turístico, viajes, vacaciones, destino, Coba Viajes">
  <PaqueteDetalle client:load apiUrl={API_URL} dominio={DOMINIO} />
</BaseLayout>
